var INITIAL_STATE = {
  activeTab: null,
  disabledTabs: ['chrono', 'time', 'client', 'project', 'info'] };




var TabReducer = exports.TabReducer = function TabReducer() {var state = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : INITIAL_STATE;var action = arguments[1];
  switch (action.type) {
    case require('../actions/types').ACTIVATE_TAB:
      var disabledTabs = [].concat(babelHelpers.toConsumableArray(state.disabledTabs));
      index = disabledTabs.findIndex(function (tab) {return tab === action.payload;});
      disabledTabs.splice(index, 1);
      return babelHelpers.extends({}, state, { activeTab: action.payload, disabledTabs: disabledTabs });
    case require('../actions/types').DISABLE_TABS:
      return babelHelpers.extends({}, state, { disabledTabs: action.payload });
    case require('../actions/types').RESET_INFO:
      return INITIAL_STATE;
    default:
      return state;}

};